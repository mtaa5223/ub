    <script>
        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function(e) {
                    $('.image-upload-wrap').hide();
                    $('#loading').show();
                    $('.file-upload-image').attr('src', e.target.result);
                    $('.file-upload-content').show();
                    $('.image-title').html(input.files[0].name);
                };
                reader.readAsDataURL(input.files[0]);
                init().then(function() {
                    console.log("hello");
                    predict();
                    $('#loading').hide();
                });
            } else {
                removeUpload();
            }
        }

        function removeUpload() {
            $('.file-upload-input').replaceWith($('.file-upload-input').clone());
            $('.file-upload-content').hide();
            $('.image-upload-wrap').show();
        }
        $('.image-upload-wrap').bind('dragover', function() {
            $('.image-upload-wrap').addClass('image-dropping');
        });
        $('.image-upload-wrap').bind('dragleave', function() {
            $('.image-upload-wrap').removeClass('image-dropping');
        });
    </script>
    <script type="text/javascript">
        let URL;
        const urlMale = "https://teachablemachine.withgoogle.com/models/o9D1N5TN/";
        const urlFemale = "https://teachablemachine.withgoogle.com/models/bB3YHn5r/";
        let model, webcam, labelContainer, maxPredictions;
        async function init() {
            if (document.getElementById("gender").checked) {
                URL = urlMale;
            } else {
                URL = urlFemale;
            }
            const modelURL = URL + "model.json";
            const metadataURL = URL + "metadata.json";
            model = await tmImage.load(modelURL, metadataURL);
            maxPredictions = model.getTotalClasses();
            labelContainer = document.getElementById("label-container");
            for (let i = 0; i < maxPredictions; i++) {
                var element = document.createElement("div")
                element.classList.add("d-flex");
                labelContainer.appendChild(element);
            }
        }
        async function predict() {
            var image = document.getElementById("face-image")
            const prediction = await model.predict(image, false);
            prediction.sort((a, b) => parseFloat(b.probability) - parseFloat(a.probability));
            console.log(prediction[0].className);
            var resultTitle, resultExplain, resultCeleb;
            if (document.getElementById("gender").checked) {
                gtag('event', '닮은똥 테스트 수행', {
                    'event_category': '닮은똥 테스트 수행',
                    'event_label': '남자'
                });
                switch (prediction[0].className) {
                    case "dog":
                        resultTitle = "귀여운 강아지똥"
                        resultExplain = "가끔은 둥글둥글하지만 거의 매일 기다란똥이 나오는 개똥!"
                        resultCeleb = "강아지똥특징 : 건강함,똑똑함,잘생김,잘난척이심함"
                        gtag('event', '남자 결과 강아지똥', {'event_category': '남자 결과'});
                        break;
                    case "cat":
                        resultTitle = "딱딱한 고양이똥"
                        resultExplain = "작고 딱딱한 고양이똥! 가끔 색깔이 노랗게 나와 모래와 섞여있으면 모른다!"
                        resultCeleb = "고양이똥특징 : 귀여움,멍청함,순진함,돼지같음"
                        gtag('event', '남자 결과 고양이똥', {'event_category': '남자 결과'});
                        break;
                    case "rabbit":
                        resultTitle = "거대한 코끼리똥"
                        resultExplain = "똥을 몇키로씩 싸는 코끼리똥 냄새도 지독하다고한다!!"
                        resultCeleb = "코끼리똥 특징 : 똥 오래동안 쌈, 치질 자주걸림, 똥냄새 심함, 똥 ㅈㄴ큼"
                        gtag('event', '남자 결과 토끼똥', {'event_category': '남자 결과'});
                        break;
                    case "dinosaur":
                        resultTitle = "알약같은 사슴똥"
                        resultExplain = "웬만한 알약과 거의 닮았다.."
                        resultCeleb = "사슴똥 특징 :편식쟁이,바보,엎드려있음,변비쟁이"
                        gtag('event', '남자 결과 공룡똥', {'event_category': '남자 결과'});
                        break;
                        case "giraffe":
                        resultTitle = "작은 기린똥"
                        resultExplain = "완두콩과 비슷한 모양이다!"
                        resultCeleb = "키큼,공부잘함,운동잘함"
                        gtag('event', '남자 결과 기린똥', {'event_category': '남자 결과'});
                        break;
                        case "tiger":
                        resultTitle = "무섭 호랑이똥"
                        resultExplain = "흡사 뺴뺴로와 비슷한 모양이다"
                        resultCeleb = "무섭게 생김, 손으로 뜯는걸 좋아함 멍청함"
                        gtag('event', '남자 결과 호랑이똥', {'event_category': '남자 결과'});
                        break;
                        case "Pigeon":
                        resultTitle = "뿌직 비둘기똥"
                        resultExplain = "하늘에서 싸서 사람들에게 똥을 맞게 해주는 흰색깔 모양의 물똥!"
                        resultCeleb = "설사많이함,아무대나 똥쌈,입작음"
                        gtag('event', '남자 결과 비둘기똥', {'event_category': '남자 결과'});
                        break;
                    default:
                        resultTitle = "알수없음"
                        resultExplain = ""
                        resultCeleb = ""
                        gtag('event', '남자 결과 알수없음', {'event_category': '남자 결과'});
                }
            } else {
                gtag('event', '동물상 테스트 수행', {
                    'event_category': '동물상 테스트 수행',
                    'event_label': '여자'
                });
                switch (prediction[0].className) {
                    case "dog":
                        resultTitle = "귀여운 강아지똥"
                        resultExplain = "가끔은 둥글둥글하지만 거의 매일 기다란똥이 나오는 개똥!"
                        resultCeleb = "강아지똥특징 : 건강함,똑똑함,잘생김,잘난척이심함"
                        gtag('event', '여자 결과 강아지똥', {'event_category': '여자 결과'});
                        break;
                    case "cat":
                        resultTitle = "딱딱한 고양이똥"
                        resultExplain = "작고 딱딱한 고양이똥! 가끔 색깔이 노랗게 나와 모래와 섞여있으면 모른다!"
                        resultCeleb = "고양이똥특징 : 귀여움,멍청함,순진함,돼지같음"
                        gtag('event', '여자 결과 고양이똥', {'event_category': '여자 결과'});
                        break;
                    case "elephant":
                        resultTitle = "거대한 코끼리똥"
                        resultExplain = "똥을 몇키로씩 싸는 코끼리똥 냄새도 지독하다고한다!"
                        resultCeleb = "코끼리똥 특징 : 똥 오래동안 쌈, 치질 자주걸림, 똥냄새 심함, 똥 ㅈㄴ큼"
                        gtag('event', '여자 결과 토끼똥', {'event_category': '여자 결과'});
                        break;
                    case "deer":
                        resultTitle = "알약같은 사슴똥"
                        resultExplain = "웬만한 알약과 거의 닮았다"
                        resultCeleb = "사슴똥 특징 :편식쟁이,바보,엎드려있음,변비쟁이"
                        gtag('event', '여자 결과 사슴똥', {'event_category': '여자 결과'});
                        break;
                    case "rabbit":
                        resultTitle = "동글동글 토끼똥"
                        resultExplain = "동글동글하고 작고 여러개의 똥이 나온다."
                        resultCeleb = "토끼똥 특징 : 편식을 많이함. 고기만먹음, 많이안먹음"
                        gtag('event', '여자 결과 사슴똥', {'event_category': '여자 결과'});
                        break;
                        case "giraffe":
                        resultTitle = "작은 기린똥"
                        resultExplain = "완두콩과 비슷한 모양이다!"
                        resultCeleb = "키큼,공부잘함,운동잘함"
                        gtag('event', '남자 결과 기린똥', {'event_category': '남자 결과'});
                        case "tiger":
                        resultTitle = "무섭 호랑이똥"
                        resultExplain = "흡사 뺴뺴로와 비슷한 모양이다"
                        resultCeleb = "무섭게 생김, 손으로 뜯는걸 좋아함 멍청함"
                        gtag('event', '남자 결과 호랑이똥', {'event_category': '남자 결과'});
                        break;
                        case "Pigeon":
                        resultTitle = "뿌직 비둘기똥"
                        resultExplain = "하늘에서 싸서 사람들에게 똥을 맞게 해주는 흰색깔 모양의 물똥!"
                        resultCeleb = "설사많이함,아무대나 똥쌈,입작음"
                        gtag('event', '남자 결과 비둘기똥', {'event_category': '남자 결과'});
                        break;
                    default:
                        resultTitle = "알수없음"
                        resultExplain = ""
                        resultCeleb = ""
                        gtag('event', '여자 결과 알수없음', {'event_category': '여자 결과'});
                }
            }
            var title = "<div class='" + prediction[0].className + "-animal-title'>" + resultTitle + "</div>"
            var explain = "<div class='animal-explain pt-2'>" + resultExplain + "</div>"
            var celeb = "<div class='" + prediction[0].className + "-animal-celeb pt-2 pb-2'>" + resultCeleb + "</div>"
            $('.result-message').html(title + explain + celeb);
            var barWidth;
            for (let i = 0; i < maxPredictions; i++) {
                if (prediction[i].probability.toFixed(2) > 0.1) {
                    barWidth = Math.round(prediction[i].probability.toFixed(2) * 100) + "%";
                } else if (prediction[i].probability.toFixed(2) >= 0.01) {
                    barWidth = "4%"
                } else {
                    barWidth = "2%"
                }
                var labelTitle;
                switch (prediction[i].className) {
                    case "dog":
                        labelTitle = "강아지똥"
                        break;
                    case "cat":
                        labelTitle = "고양이똥"
                        break;
                    case "rabbit":
                        labelTitle = "토끼똥"
                        break;
                    case "giraffe":
                        labelTitle = "기린똥"
                        break;
                    case "deer":
                        labelTitle = "사슴똥"
                        break;
                }
                var label = "<div class='animal-label d-flex align-items-center'>" + labelTitle + "</div>"
                var bar = "<div class='bar-container position-relative container'><div class='" + prediction[i].className + "-box'></div><div class='d-flex justify-content-center align-items-center " + prediction[i].className + "-bar' style='width: " + barWidth + "'><span class='d-block percent-text'>" + Math.round(prediction[i].probability.toFixed(2) * 100) + "%</span></div></div>"
                labelContainer.childNodes[i].innerHTML = label + bar;
            }
        }
    </script>
    <script>
        function iosApp() {
            document.getElementById("coupang").style.display="none";
            $("#coupang ins").hide();
            document.getElementById("addThis").style.display="none";
            document.getElementById("disqus_thread").style.display="none";
            var kakao = document.getElementsByClassName("kakao_ad_area");
            for (var i = 0; i < kakao.length; i++) {
              if(kakao[i]) {
                  kakao[i].style.display="none";
              }
            }
            document.getElementById("yotube-top-link").style.display="none";
            document.getElementById("yotube-mid-link").style.display="none";
            document.getElementById("yotube-bottom-link").style.display="none";
        }
    </script>
</body>
